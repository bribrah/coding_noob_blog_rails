<!DOCTYPE html>
<html>
  <head>
    <%= render  "layouts/head" %>
  </head>

  <body>
    <%= render  "layouts/navbar" %>
    <div class="container">
        <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %>"> <%= message %></div>
      <% end %>
      <%= yield %>
      <%= render  "layouts/footer" %>
  </div>
  </body>
</html>
